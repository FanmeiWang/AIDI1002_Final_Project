import os
import pandas as pd

DATA_PATH = r'C:\Users\HP\Desktop\Main Project\data\ibm.csv'

print(f"Checking file path: {DATA_PATH}")
print(f"File exists: {os.path.exists(DATA_PATH)}")  # Check if file exists

# Now load the data
try:
    data = pd.read_csv(DATA_PATH)
    print("Data loaded successfully!")
    print(data.head())
except Exception as e:
    print(f"Error loading data: {e}")
